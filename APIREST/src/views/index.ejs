<!DOCTYPE html>
<html lang="en">
<head>
   <meta charset="UTF-8">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <title>APIREST</title>
   <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">

</head>
<body>
   <br>
   <div class="container">
      <a href="/addconsole" type="button" class="btn btn-success">Agregar consola</a>
   </div>
   <br>
   <table class="table">
      <thead class="thead-dark">
        <tr>
          <th scope="col">ID</th>
          <th scope="col">Nombre</th>
          <th scope="col">Empresa</th>
          <th scope="col">Precio</th>
          <th scope="col">Descripcion</th>
          <th scope="col">Ram</th>
          <th scope="col">Acciones</th>
        </tr>
      </thead>
      
      <tbody>
         <% for(var i = 0; i < data.length; i++) { %>
            <tr>
               <th scope="row"><%= data[i].id %></th>
               <td><%= data[i].nombre %></td>
               <td><%= data[i].empresa %></td>
               <td><%= data[i].precio %></td>
               <td><%= data[i].descripcion %></td>
               <td><%= data[i].ram %></td>
               <td>
                  <a href="/delete/<%= data[i].id %>" type="button" class="btn btn-danger">Eliminar</a>
                  <a href="/pre_edit/<%= data[i].id %>" type="button" class="btn btn-primary">Editar</a>
               </td>
            </tr>
        <%  } %>
      </tbody>
    </table>
    
    
      

</body>
</html>